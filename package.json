{
  "name": "@bolajiolajide/now-playing",
  "version": "1.0.0",
  "description": "A simple package to get the current playing song from your user streaming platform",
  "homepage": "https://github.com/bolajiolajide/now-playing",
  "main": "dist/now-playing.mjs",
  "module": "dist/now-playing.mjs",
  "types": "dist/now-playing.d.ts",
  "type": "module",
  "scripts": {
    "build": "npx rimraf dist && vite build",
    "test": "vitest",
    "format": "prettier . --write"
  },
  "exports": {
    "types": "./dist/now-playing.d.ts",
    "default": "./dist/now-playing.mjs"
  },
  "files": [
    "dist/now-playing.mjs",
    "dist/now-playing.d.ts"
  ],
  "keywords": ["spotify", "now-playing", "in-memory"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "2.6.7",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "11.1.6",
    "@types/node": "18.19.39",
    "@types/node-fetch": "2.6.11",
    "msw": "2.3.1",
    "prettier": "3.3.2",
    "tslib": "2.6.3",
    "typescript": "5.4.5",
    "vite": "^5.3.3",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "1.6.0"
  },
  "engines": {
    "node": ">=18"
  }
